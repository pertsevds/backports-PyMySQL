[run]
branch = True
source =
    backports_3_7/pymysql
omit = backports_3_7/pymysql/tests/*
       backports_3_7/pymysql/tests/thirdparty/test_MySQLdb/*

[report]
exclude_lines =
    pragma: no cover
    except ImportError:
    if DEBUG:
    def __repr__
    def __str__
    raise NotImplementedError
    def __getattr__
    raise ValueError
